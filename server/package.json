{
  "name": "react-node-boilerplate",
  "version": "1.0.0",
  "description": "A React Js and Node Js boilerplate for building a new project. It uses create-react-app",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "server": "nodemon server.js",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "engines": {
    "node": "10.10.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-appstream": "^3.42.0",
    "aws-sdk": "^2.1037.0",
    "body-parser": "^1.19.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.16.3",
    "express-basic-auth": "^1.2.0",
    "mongoose": "^5.7.5",
    "multer": "^1.4.0",
    "multer-s3": "^2.7.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.1",
    "net": "^1.0.2",
    "nodemon": "^2.0.13",
    "path": "^0.12.7",
    "react-validation": "^3.0.7",
    "sequelize": "^6.7.0",
    "url": "^0.11.0",
    "validation": "0.0.1",
    "validator": "^10.8.0"
  }
}
